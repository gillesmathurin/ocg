<div class="yui-g sliderfalse">
	<h1 class="entete1">Listing des videos</h1>
	
	
	<table class="videos">
	<% @videos.each do |video| %>
	  <tr>
	    <td>
					<%= video.youtube_url %>
					<p class="video-desc"><%= video.description %></p>
					<p>
						<% if user_signed_in? %>
					    <%= link_to 'Modifier', edit_video_path(video) %> |
					    <%= link_to 'Supprimer', video, :confirm => 'En Ãªtes vous sur?', :method => :delete %>
						<% end %>
					</p>
			</td>		
	  </tr>
	<% end %>	
	</table>

	<br />

	<%= link_to 'Ajouter une video', new_video_path %>
</div>
