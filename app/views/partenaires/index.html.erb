<h1>Listing partenaires</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Adresse</th>
    <th>Cp</th>
    <th>Ville</th>
    <th>Telephone</th>
    <th>Fax</th>
    <th>Email</th>
    <th>Weburl</th>
    <th>Logo file name</th>
    <th>Logo file size</th>
    <th>Logo content type</th>
    <th>Logo updated at</th>
  </tr>

<% @partenaires.each do |partenaire| %>
  <tr>
    <td><%=h partenaire.name %></td>
    <td><%=h partenaire.adresse %></td>
    <td><%=h partenaire.cp %></td>
    <td><%=h partenaire.ville %></td>
    <td><%=h partenaire.telephone %></td>
    <td><%=h partenaire.fax %></td>
    <td><%=h partenaire.email %></td>
    <td><%=h partenaire.weburl %></td>
    <td><%=h partenaire.logo_file_name %></td>
    <td><%=h partenaire.logo_file_size %></td>
    <td><%=h partenaire.logo_content_type %></td>
    <td><%=h partenaire.logo_updated_at %></td>
    <td><%= link_to 'Show', partenaire %></td>
    <td><%= link_to 'Edit', edit_partenaire_path(partenaire) %></td>
    <td><%= link_to 'Destroy', partenaire, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New partenaire', new_partenaire_path %>