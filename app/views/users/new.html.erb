<div class="yui-g sliderfalse">
	<h1 class="entete1">Enregistrez-vous</h1>
	<% form_for @user, :url => user_path do |f| -%>
	  <%= f.error_messages %>
		<p><%= f.label :email, "Email" %></p>
		<p><%= f.text_field :email %></p>
		<p><%= f.label :password, "Password" %></p>
		<p><%= f.password_field :password %></p>
		<p><%= f.label :password_confirmation, "Password Confirmation" %></p>
		<p><%= f.password_field :password_confirmation %></p>
		
		<p class="submit"><%= f.submit "Enregistrement" %></p>
	<% end -%>
</div>

<div class="yui-gb maincontent">
	<div class="yui-u first">

	</div>
	
	<div class="yui-u">

	</div>

</div>